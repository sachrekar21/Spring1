{
    "family": "PetClinic",
    "containerDefinitions": [
        {
            "image": "293329462814.dkr.ecr.us-east-1.amazonaws.com/petclinic/config-server",
            "name": "config-server",
            "cpu": 256,
            "memory": 1024,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 8888,
                    "hostPort": 8888
                }]
        },
		{
            "image": "293329462814.dkr.ecr.us-east-1.amazonaws.com/petclinic/discovery-server",
            "name": "discovery-server",
            "cpu": 256,
            "memory": 1024,
            "essential": true,
            "entryPoint": ["./wait-for-it.sh","config-server:8888","--timeout=60","--","java","-Djava.security.egd=file:/dev/./urandom","-jar","/app.jar"],
            "portMappings": [
                {
                    "containerPort": 8761,
                    "hostPort": 8761
                }]
        },
		{
            "image": "293329462814.dkr.ecr.us-east-1.amazonaws.com/petclinic/customers-service",
            "name": "customers-service",
